{
  "manifest_version": 3,
  "name": "Navigator",
  "version": "0.1.0",
  "description": "智能书签管理工具 - 主页网格布局、AI 增强、本地存储",
  "permissions": [
    "storage",
    "bookmarks",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_title": "Navigator",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png"
    }
  }
}
