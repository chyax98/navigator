{
  "name": "navigator",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "dev:ext": "vite --config vite.config.extension.ts --mode extension",
    "build:ext": "vue-tsc && vite build --config vite.config.extension.ts --mode extension",
    "pack:ext": "npm run build:ext && cd dist-extension && zip -r ../navigator-extension.zip ."
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.13.0",
    "@vueuse/core": "^10.7.0",
    "dayjs": "^1.11.10",
    "dompurify": "^3.3.0",
    "fuse.js": "^7.0.0",
    "marked": "^16.4.1",
    "naive-ui": "^2.38.0",
    "nanoid": "^5.0.0",
    "openai": "^6.7.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.2.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.10.0",
    "@types/sortablejs": "^1.15.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.0",
    "happy-dom": "^14.11.3",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^3.1.2"
  }
}
